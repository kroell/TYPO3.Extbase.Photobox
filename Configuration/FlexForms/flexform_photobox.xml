<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Select function</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Photobox</numIndex>
										<numIndex index="1">Photobox->list</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					<settings.showThumbnails>
				      <TCEforms>
					    <exclude>1</exclude>
						<label>Thumbnails anzeigen</label>
						<config>
						  <type>check</type>
						</config>
					  </TCEforms>
					</settings.showThumbnails>
					<settings.loop>
				      <TCEforms>
					    <exclude>1</exclude>
						<label>Loop</label>
						<config>
						  <type>check</type>
						</config>
					  </TCEforms>
					</settings.loop>
					<settings.autoplay>
				      <TCEforms>
					    <exclude>1</exclude>
						<label>Autoplay</label>
						<config>
						  <type>check</type>
						</config>
					  </TCEforms>
					</settings.autoplay>
					<settings.timeAutoplay>
				      <TCEforms>
					    <exclude>1</exclude>
						<label>Autoplay Geschwindigkeit (ms)</label>
						<config>
						  <type>input</type>
						  <size>20</size>
						  <max>30</max>
						  <eval>trim</eval>
						</config>
					  </TCEforms>
					</settings.timeAutoplay>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>